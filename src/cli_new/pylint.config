[MASTER]
ignore=.git

[MESSAGES CONTROL]
disable=I0011,no-self-use,fixme

[REPORTS]
reports=no

[BASIC]
bad-functions=input,file
good-names=i,j,k,f,ip,_
min-public-methods=0

[FORMAT]
max-line-length=80
indent-string='    '

[DESIGN]
max-returns=100

[EXCEPTIONS]
overgeneral-exceptions=

[TYPECHECK]
ignored-modules = netifaces
